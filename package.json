{
  "name": "drafty",
  "version": "1.0.0",
  "description": "A discord bot that helps to organize Magic The Gathering draft pods",
  "packageManager": "pnpm@8.6.1",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "pnpm -r --include-workspace-root exec rm -rf node_modules && rm -rf $(pnpm store path)",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "dev:bot": "cd bot && pnpm dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkergal/drafty.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jkergal/drafty/issues"
  },
  "homepage": "https://github.com/jkergal/drafty#readme",
  "devDependencies": {
    "prettier": "^2.8.4"
  }
}
