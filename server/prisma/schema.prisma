generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model drafty_configurations {
  id                 Int       @id
  created_at         DateTime? @db.Date
  scheduled_message  String?   @db.VarChar
  checkin_message    String?   @db.VarChar
  cron_date          String?   @db.VarChar
  current_mtg_format String?   @db.VarChar
}

model enrollment_messages {
  id         Int       @id
  sent_at    DateTime? @db.Date
  discord_id String?   @db.VarChar
  pods       pods?
}

model game_tables {
  id                  Int       @id
  completed_at        DateTime? @db.Date
  player_1_discord_id String?   @db.VarChar
  player_2_discord_id String?   @db.VarChar
  player_3_discord_id String?   @db.VarChar
  player_4_discord_id String?   @db.VarChar
  player_5_discord_id String?   @db.VarChar
  player_6_discord_id String?   @db.VarChar
  player_7_discord_id String?   @db.VarChar
  player_8_discord_id String?   @db.VarChar
  pods                pods      @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model pods {
  id                  Int                 @id
  starts_at           DateTime?           @db.Date
  game_tables         game_tables?
  enrollment_messages enrollment_messages @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
